<%= form_for(job) do |f| %>
  
  <div class="field">
    <%= f.label :task_id %>
    <%= f.number_field :task_id %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.number_field :user_id %>
  </div>

  <div class="field">
    <%= f.label :performed_at %>
    <%= f.date_select :performed_at %>
  </div>

  <div class="field">
    <%= f.label :comment %>
    <%= f.text_field :comment %>
  </div>

  <div class="field">
    <%= f.label :time %>
    <%= f.text_field :time %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
